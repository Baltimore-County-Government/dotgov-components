{
	"name": "@baltimorecounty/dotgov-components",
	"version": "0.0.11",
	"description": "UI design system for Baltimore County's primary [website](https://www.baltimorecountymd.gov).",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"css",
		"es",
		"lib",
		"umd"
	],
	"scripts": {
		"build": "styleguidist build",
		"build-demo": "nwb build-react-component",
		"clean-demo": "nwb clean-module && nwb clean-demo",
		"start-demo": "nwb serve-react-dem",
		"prepublishOnly": "npm run build-demo",
		"start": "styleguidist server",
		"test": "nwb test-react",
		"test:coverage": "nwb test-react --coverage",
		"test:watch": "nwb test-react --server",
		"predeploy": "npm run-script build",
		"deploy": "gh-pages -d styleguide --git git"
	},
	"dependencies": {},
	"peerDependencies": {
		"react": "16.x",
		"react-dom": "16.x"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.5",
		"gh-pages": "^2.1.0",
		"node-sass": "^4.12.0",
		"nwb": "0.23.x",
		"react-styleguidist": "^9.1.13",
		"sass-loader": "^7.1.0",
		"webpack": "^4.39.1"
	},
	"author": "",
	"homepage": "https://github.com/baltimorecounty/dotgov-components",
	"license": "MIT",
	"repository": "https://github.com/baltimorecounty/dotgov-components.git",
	"keywords": [
		"react-component"
	],
	"browserslist": [
		">1%",
		"last 1 version",
		"Firefox ESR",
		"not dead"
	],
	"publishConfig": {
		"access": "public"
	}
}
